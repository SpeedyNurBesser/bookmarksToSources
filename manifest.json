{
    "manifest_version": 2,
    "name": "BookmarksToSources",
    "version": "1.0.0",
    "description": "Generate a sources document from your bookmarks!",
    "permissions": [
        "bookmarks",
        "tabs"
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "browser_action": {},
    "content_security_policy": "script-src 'self' https://code.getmdl.io/1.3.0/material.min.js https://cdn.jsdelivr.net/npm/treeselectjs@0.12.2/dist/treeselectjs.umd.js; object-src 'self';"
}